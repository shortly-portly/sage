<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "company-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= input f, :name %>
  <%= input f, :address_line_1 %>
  <%= input f, :address_line_2 %>
  <%= input f, :county %>
  <%= input f,  :postcode %>
  <%= input f, :country %>
  <%= input f, :telephone %>
  <%= input f, :email %>
  <%= input f, :website %>

  <%= my_submit "Save", phx_disable_with: "Saving..." %>

</form>
